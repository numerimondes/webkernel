{
    "name": "webkernel/platform-config",
    "description": "Webkernel Platform Configuration with Dynamic Constants and Class Aliases",
    "type": "library",
    "authors": [
        {
            "name": "El Moumen Yassine",
            "email": "yassine@numerimondes.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webkernel\\PlatformConfig\\": "packages/webkernel/src/PlatformConfig/"
        },
        "files": [
            "packages/webkernel/src/PlatformConfig/ConfigFiles/WebkernelStaticGeneratedFiles/constants-globals.php",
            "packages/webkernel/src/PlatformConfig/ConfigFiles/WebkernelStaticGeneratedFiles/autoload-stubs.php"
        ]
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkernel\\Core\\Providers\\WebkernelConstantsServiceProvider"
            ]
        }
    }
}